<html>
<head>
<script>
	function afficher(){
		document.write("<table border=2>");
		for(var i=1;i<=10;i++){
			document.write("<tr>");
			for(var j=1;j<=10;j++){
				document.write("<td>*</td>");
			}
			document.write("</tr>");
		}
		
	}
</script>
</head>
<body onload="afficher()">

</body>
</html>