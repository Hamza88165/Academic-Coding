<html>
<head>
<script>
	function afficher(){
		document.write("<h1>Titre:1colonne 100 rows</h1>");
		document.write("<table border=2>");
		for(var i=1;i<=100;i++){
			document.write("<tr><td>"+i+"</td></tr>");
		}
		document.write("</table>");
	}
</script>
</head>
<body onload="afficher()">

</body>
</html>