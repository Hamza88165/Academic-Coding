<html>
<head>
<script>
	function afficher(){
		document.write('<table border=2>');
		document.write('<tr>');
		document.write('<td>*</td>');
		document.write('<td>*</td>');
		document.write('<td>*</td>');
		document.write('<td>*</td>');
		document.write('</tr>');
		document.write('</table>');
	}
</script>
</head>
<body onload="afficher()">
<!-- 				explaination 				-->
<!-- document.write():Write some text directly 
			to the HTML output -->
<!-- onload event: Execute a JavaScript 
					immediately after a page has been loaded -->
</body>
</html>